<%- include('partials/head') %>

<body>
  <%- include('partials/header') %>

  <div class="container py-md-5 container--narrow">

  <!--Show registration errors-->
  <%- include('partials/flash') %>

  <%- include('partials/profile-shared') %>

  <div class="list-group">
    <% following.forEach(function(follow) { %>
      <a href="/profile/<%= follow.username %>" class="list-group-item list-group-item-action">
        <img class="avatar-tiny" src="<%= follow.avatar %>">
        <%= follow.username %>
    </a>
    <% }) %>
  </div>
  </div>

  <%- include('partials/footer') %>